<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified" targetNamespace="http://www.creditsafe.com/globaldata/datatypes/administration" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:mstns="http://www.creditsafe.com/globaldata/datatypes/administration" xmlns="http://www.creditsafe.com/globaldata/datatypes/administration">
  <xs:complexType name="CurrentUserAccountAccessRights">
    <xs:complexContent mixed="false">
      <xs:extension base="q1:ServiceResponse" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes">
        <xs:sequence>
          <xs:element minOccurs="0" name="AccessRights" type="UserAccountAccessRights"/>
        </xs:sequence>
        <xs:attribute name="Id" type="xs:unsignedInt" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CurrentProviderServicesSettings">
    <xs:complexContent mixed="false">
      <xs:extension base="q1:ServiceResponse" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes">
        <xs:sequence>
          <xs:element minOccurs="0" name="Settings" type="ProviderServicesSettings"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CurrentDataSourceSettings">
    <xs:complexContent mixed="false">
      <xs:extension base="q1:ServiceResponse" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes">
        <xs:sequence>
          <xs:element minOccurs="0" name="Settings" type="DataSourceSettings"/>
        </xs:sequence>
        <xs:attribute name="Id" type="xs:unsignedInt" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="UserAccountActivityLog">
    <xs:complexContent mixed="false">
      <xs:extension base="q1:ServiceResponse" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes">
        <xs:sequence>
          <xs:element minOccurs="0" name="Browsing">
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="LogEntry" type="ActivityLogBrowsingEntry"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="OnlineReports">
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="LogEntry" type="ActivityLogReportEntry"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="OfflineReports">
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="LogEntry" type="ActivityLogReportEntry"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="Monitoring">
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="LogEntry" type="ActivityLogMonitoringEntry"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
        <xs:attribute name="Id" type="xs:unsignedInt" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SystemStatusSummary">
    <xs:complexContent mixed="false">
      <xs:extension base="q1:ServiceResponse" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes">
        <xs:sequence>
          <xs:element minOccurs="0" name="Overview" type="StatusOverview"/>
          <xs:element minOccurs="0" name="Subsystems">
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="Subsystem" type="SystemStatusSummary.SubsystemReport"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DataSourcesReport">
    <xs:complexContent mixed="false">
      <xs:extension base="q1:ServiceResponse" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes">
        <xs:sequence>
          <xs:element name="DataSources">
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="DataSource" type="DataSource"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="Priorities">
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="Priority" type="DataSourcePriorities"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="FullSystemReport">
    <xs:complexContent mixed="false">
      <xs:extension base="q1:ServiceResponse" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes">
        <xs:sequence>
          <xs:element minOccurs="0" name="Overview" type="StatusOverview"/>
          <xs:element minOccurs="0" name="Subsystems">
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="Subsystem" type="FullSystemReport.SubsystemReport"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="Log" type="Log"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CurrentCustomerSettings">
    <xs:complexContent mixed="false">
      <xs:extension base="q1:ServiceResponse" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes">
        <xs:sequence>
          <xs:element minOccurs="0" name="Settings" type="CustomerSettings"/>
        </xs:sequence>
        <xs:attribute name="Id" type="xs:unsignedInt" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="UserAccountsList">
    <xs:complexContent mixed="false">
      <xs:extension base="q1:ServiceResponse" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="UserAccount" type="UserAccountOverview"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CustomersList">
    <xs:complexContent mixed="false">
      <xs:extension base="q1:ServiceResponse" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes">
        <xs:sequence>
          <xs:element minOccurs="0" maxOccurs="unbounded" name="Customer" type="CustomerData"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="CurrentUserAccountSettings">
    <xs:complexContent mixed="false">
      <xs:extension base="q1:ServiceResponse" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes">
        <xs:sequence>
          <xs:element minOccurs="0" name="Settings" type="UserAccountSettings"/>
        </xs:sequence>
        <xs:attribute name="Id" type="xs:unsignedInt" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="UserAccountAccessRights">
    <xs:sequence>
      <xs:element minOccurs="0" name="Browsing">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="ACE" type="AccessControlEntry"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="OnlineReports">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="ACE" type="AccessControlEntry"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="OfflineReports">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="ACE" type="AccessControlEntry"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="Monitoring">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="ACE" type="AccessControlEntry"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ProviderServicesSettings">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Service" type="ProviderServiceSettings"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DataSourceSettings">
    <xs:sequence>
      <xs:element name="Countries">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="Country" type="CountrySettings"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="ReportPricing" type="ReportPricing"/>
    </xs:sequence>
    <xs:attribute name="Enabled" type="xs:boolean" use="required"/>
  </xs:complexType>
  <xs:complexType name="ActivityLogBrowsingEntry">
    <xs:complexContent mixed="false">
      <xs:extension base="ActivityLogEntry">
        <xs:sequence>
          <xs:element name="Countries">
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="Country" type="q1:CountryCode" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="SearchCriteria" type="q1:SearchCriteria" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ActivityLogReportEntry">
    <xs:complexContent mixed="false">
      <xs:extension base="ActivityLogEntry">
        <xs:sequence>
          <xs:element name="CompanyId" type="xs:string"/>
          <xs:element name="Country" type="q1:CountryCode" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes"/>
          <xs:element name="ReportType" type="q1:CompanyReportType" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes"/>
          <xs:element minOccurs="0" name="CompanyName" type="xs:string"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ActivityLogMonitoringEntry">
    <xs:complexContent mixed="false">
      <xs:extension base="ActivityLogEntry">
        <xs:sequence>
          <xs:element name="PortfolioName" type="xs:string"/>
          <xs:element name="Addition" type="xs:boolean"/>
          <xs:element minOccurs="0" name="Companies">
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" maxOccurs="unbounded" name="Company" type="xs:string"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="StatusOverview">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" name="Country" type="CountryStatus"/>
    </xs:sequence>
    <xs:attribute name="Status" type="StatusFlags" use="required"/>
    <xs:attribute name="Hash" type="xs:string" use="required"/>
  </xs:complexType>
  <xs:complexType name="SystemStatusSummary.SubsystemReport">
    <xs:complexContent mixed="false">
      <xs:extension base="SystemStatusSummary">
        <xs:attribute name="Id" type="xs:unsignedInt" use="required"/>
        <xs:attribute name="Name" type="xs:string" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DataSource">
    <xs:sequence>
      <xs:element minOccurs="0" name="CountriesEnabled">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="EnabledCountry">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:boolean">
                    <xs:attribute name="key" type="q1:CountryCode" use="required" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes"/>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
        <xs:unique name="Creditsafe.GlobalData.Administration.DataSource-CountriesEnabled">
          <xs:selector xpath="mstns:EnabledCountry"/>
          <xs:field xpath="@key"/>
        </xs:unique>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="Id" type="xs:unsignedInt" use="required"/>
    <xs:attribute name="Name" type="xs:string" use="required"/>
    <xs:attribute name="Enabled" type="xs:boolean" use="required"/>
  </xs:complexType>
  <xs:complexType name="DataSourcePriorities">
    <xs:sequence>
      <xs:element minOccurs="2" maxOccurs="unbounded" name="DS" type="xs:unsignedInt"/>
    </xs:sequence>
    <xs:attribute name="Country" type="q1:CountryCode" use="required" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes"/>
  </xs:complexType>
  <xs:complexType name="FullSystemReport.SubsystemReport">
    <xs:complexContent mixed="false">
      <xs:extension base="FullSystemReport">
        <xs:attribute name="Id" type="xs:unsignedInt" use="required"/>
        <xs:attribute name="Name" type="xs:string" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Log">
    <xs:sequence>
      <xs:element minOccurs="0" name="Messages">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="Message" type="TextLogEntry"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="LoadHistory">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="LoadHistoryItem" type="LoadLogEntry"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="Overloads">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="Overload">
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" maxOccurs="unbounded" name="item" type="TextLogEntry"/>
                </xs:sequence>
                <xs:attribute name="key" type="xs:string" use="required"/>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
        <xs:unique name="Creditsafe.GlobalData.Administration.Log-Overloads">
          <xs:selector xpath="mstns:Overload"/>
          <xs:field xpath="@key"/>
        </xs:unique>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CustomerSettings">
    <xs:sequence>
      <xs:element name="Name" type="xs:string"/>
      <xs:element name="OwningEntity" type="CreditsafeEntity"/>
      <xs:element minOccurs="0" name="AccountManager" type="xs:string"/>
      <xs:element name="Status" type="CustomerStatus"/>
      <xs:element minOccurs="0" name="Country" type="q1:CountryCode" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes"/>
      <xs:element minOccurs="0" name="Address" type="q1:AddressData" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes"/>
      <xs:element minOccurs="0" name="PhoneNumber" type="xs:string"/>
      <xs:element minOccurs="0" name="FrontSystemNumber" type="xs:string"/>
      <xs:element minOccurs="0" name="BackOfficeId" type="xs:string"/>
      <xs:element name="IsReseller" type="xs:boolean"/>
      <xs:element minOccurs="0" name="ContractStartDate" type="xs:dateTime"/>
      <xs:element minOccurs="0" name="ContractEndDate" type="xs:dateTime"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="UserAccountOverview">
    <xs:sequence>
      <xs:element name="Username" type="xs:string"/>
      <xs:element minOccurs="0" name="Email" type="xs:string"/>
      <xs:element minOccurs="0" name="ContractType" type="ContractType"/>
      <xs:element minOccurs="0" name="ParentProduct" type="ExternalProduct"/>
      <xs:element minOccurs="0" name="LastLogin" type="xs:dateTime"/>
      <xs:element minOccurs="0" name="BannedTill" type="xs:dateTime"/>
      <xs:element minOccurs="0" name="Notes" type="xs:string"/>
    </xs:sequence>
    <xs:attribute name="Id" type="xs:unsignedInt" use="required"/>
    <xs:attribute name="Enabled" type="xs:boolean" use="required"/>
    <xs:attribute name="CustomerId" type="xs:unsignedInt"/>
  </xs:complexType>
  <xs:complexType name="CustomerData">
    <xs:complexContent mixed="false">
      <xs:extension base="CustomerSettings">
        <xs:attribute name="Id" type="xs:unsignedInt" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="UserAccountSettings">
    <xs:sequence>
      <xs:element minOccurs="0" name="Username" type="xs:string"/>
      <xs:element minOccurs="0" name="Password" type="xs:string"/>
      <xs:element name="Enabled" type="xs:boolean"/>
      <xs:element minOccurs="0" name="CustomerId" type="xs:unsignedInt"/>
      <xs:element minOccurs="0" name="Email" type="xs:string"/>
      <xs:element minOccurs="0" name="ContractType" type="ContractType"/>
      <xs:element minOccurs="0" name="ParentProduct" type="ExternalProduct"/>
      <xs:element minOccurs="0" name="BannedTill" type="xs:dateTime"/>
      <xs:element minOccurs="0" name="Notes" type="xs:string"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AccessControlEntry">
    <xs:sequence>
      <xs:element minOccurs="0" name="Countries">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="Country" type="q1:CountryCode" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="CompanyType" type="q1:CompanyType" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes"/>
      <xs:element minOccurs="0" name="Message" type="xs:string"/>
    </xs:sequence>
    <xs:attribute name="AllowAccess" type="xs:boolean" use="required"/>
    <xs:attribute name="Starts" type="xs:dateTime"/>
    <xs:attribute name="Expires" type="xs:dateTime"/>
    <xs:attribute name="Limit" type="xs:unsignedInt"/>
    <xs:attribute name="CurrentCount" type="xs:unsignedInt"/>
  </xs:complexType>
  <xs:complexType name="ProviderServiceSettings">
    <xs:sequence>
      <xs:element minOccurs="1" maxOccurs="unbounded" name="Endpoint" type="Endpoint"/>
    </xs:sequence>
    <xs:attribute name="Name" type="xs:string" use="required"/>
    <xs:attribute name="Enabled" type="xs:boolean" use="required"/>
  </xs:complexType>
  <xs:complexType name="CountrySettings">
    <xs:sequence>
      <xs:element minOccurs="0" name="ReportPricing" type="ReportPricing"/>
    </xs:sequence>
    <xs:attribute name="CountryCode" type="q1:CountryCode" use="required" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes"/>
    <xs:attribute name="Enabled" type="xs:boolean" use="required"/>
  </xs:complexType>
  <xs:complexType name="ReportPricing">
    <xs:sequence>
      <xs:element minOccurs="0" name="Online">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="OnlineItem">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:decimal">
                    <xs:attribute name="key" type="q1:CompanyReportType" use="required" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes"/>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
        <xs:unique name="Creditsafe.GlobalData.Administration.ReportPricing-Online">
          <xs:selector xpath="mstns:OnlineItem"/>
          <xs:field xpath="@key"/>
        </xs:unique>
      </xs:element>
      <xs:element minOccurs="0" name="Offline">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="OfflineItem">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:decimal">
                    <xs:attribute name="key" type="q1:CompanyReportType" use="required" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes"/>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
        <xs:unique name="Creditsafe.GlobalData.Administration.ReportPricing-Offline">
          <xs:selector xpath="mstns:OfflineItem"/>
          <xs:field xpath="@key"/>
        </xs:unique>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ActivityLogEntry">
    <xs:sequence>
      <xs:element minOccurs="0" name="CustomData">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="Entry">
              <xs:complexType>
                <xs:simpleContent>
                  <xs:extension base="xs:string">
                    <xs:attribute name="key" type="xs:string" use="required"/>
                  </xs:extension>
                </xs:simpleContent>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
        <xs:unique name="Creditsafe.GlobalData.Administration.ActivityLogEntry-CustomData">
          <xs:selector xpath="mstns:Entry"/>
          <xs:field xpath="@key"/>
        </xs:unique>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="OrderNumber" type="xs:unsignedLong" use="required"/>
    <xs:attribute name="ChargeReference" type="xs:string"/>
    <xs:attribute name="Date" type="xs:dateTime" use="required"/>
    <xs:attribute name="Ip" type="xs:string" use="required"/>
    <xs:attribute name="Allowed" type="xs:boolean" use="required"/>
    <xs:attribute name="ProcessingTime" type="xs:duration"/>
  </xs:complexType>
  <xs:simpleType name="StatusFlags">
    <xs:restriction base="xs:string">
      <xs:whiteSpace value="collapse"/>
      <xs:pattern value="((None|HasWarnings|HasErrors|Online)(\s*,\s*(?!$))?)*"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="CountryStatus">
    <xs:attribute name="Country" type="q1:CountryCode" use="required" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes"/>
    <xs:attribute name="Status" type="StatusFlags" use="required"/>
  </xs:complexType>
  <xs:complexType name="TextLogEntry">
    <xs:simpleContent>
      <xs:extension base="xs:string">
        <xs:attribute name="Type" type="q1:MessageType" use="required" xmlns:q1="http://www.creditsafe.com/globaldata/datatypes"/>
        <xs:attribute name="Time" type="xs:dateTime" use="required"/>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>
  <xs:complexType name="LoadLogEntry">
    <xs:attribute name="HitsSincePrevious" type="xs:unsignedInt" use="required"/>
    <xs:attribute name="Time" type="xs:dateTime" use="required"/>
  </xs:complexType>
  <xs:simpleType name="CreditsafeEntity">
    <xs:restriction base="xs:string">
      <xs:whiteSpace value="collapse"/>
      <xs:enumeration value="INT"/>
      <xs:enumeration value="CSUK"/>
      <xs:enumeration value="CSDE"/>
      <xs:enumeration value="CSFR"/>
      <xs:enumeration value="CSNL"/>
      <xs:enumeration value="CSBE"/>
      <xs:enumeration value="CSIE"/>
      <xs:enumeration value="CSSE"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="CustomerStatus">
    <xs:restriction base="xs:string">
      <xs:whiteSpace value="collapse"/>
      <xs:enumeration value="Disabled"/>
      <xs:enumeration value="Active"/>
      <xs:enumeration value="Inactive"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ContractType">
    <xs:restriction base="xs:string">
      <xs:whiteSpace value="collapse"/>
      <xs:enumeration value="PrePaid"/>
      <xs:enumeration value="PayAsYouGo"/>
    </xs:restriction>
  </xs:simpleType>
  <xs:simpleType name="ExternalProduct">
    <xs:restriction base="xs:string">
